B = Bottom;
R = Right;

for(y=1; y<=B; y++){
  if([1,y] == "S"){
    Sjis();
  }else if([1,y] == "E"){
    Euc();
  }else if([1,y] == "J"){
    Jis();
  }else if([1,y] == "8"){
    Utf8();
  }else if([1,y] == "L"){
    Unicode();
  }else if([1,y] == "B"){
    Utf16be();
  }else if([1,y] == "T"){
    ShowToolbar();
  }else if([1,y] == "I"){
    InputBox("hey!");
  }
  
  for(x=2; x<=R; x++){
    writeln([x,y]);
  }
}
